{
  "name": "advent-of-code-ts",
  "version": "0.9.1",
  "description": "adventOfCode-TS",
  "scripts": {
    "test": "jest",
    "format": "prettier --config ./.prettierrc --write \"./src/**.js\" \"./src/**/*.ts\""
  },
  "keywords": [],
  "author": "Mario Buonomo",
  "dependencies": {
    "decorators-utils-ts": "^1.0.5",
    "dotless": "^1.0.0-beta.12",
    "double-ended-queue": "^2.1.0-0",
    "express": "^4.17.1",
    "fp-ts": "^2.2.0",
    "fs": "0.0.2",
    "line-reader": "^0.4.0",
    "lodash": "^4.17.15",
    "n-readlines": "^1.0.0",
    "react": "^16.12.0",
    "babel-jest": "^24.9.0",
    "jest-transform-stub": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.14",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2",
    "typings-checker": "^2.0.0",
    "ts-jest": "^24.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy",
      "^(app/.+)$": "<rootDir>/src/$1/",
      "^(components/.+)$": "<rootDir>/src/$1/",
      "^(stores/.+)$": "<rootDir>/src/$1/",
      "^(views/.+)$": "<rootDir>/src/$1/",
      "^(assets/.+)$": "<rootDir>/src/$1/",
      "^(models/.+)$": "<rootDir>/src/$1/"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  }
}
